<Window x:Class="QrScaner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QrScaner"
        mc:Ignorable="d"
        Title="QrTools" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Image Name="QrImage" HorizontalAlignment="Left" Height="399" Margin="10,10,0,0" VerticalAlignment="Top" Width="399" Source="Scanner.png"/>
        <Button Name="CreateButton" Content="Create" HorizontalAlignment="Left" Margin="472,334,0,0" VerticalAlignment="Top" Width="75" Click="Create_Click"/>
        <Button Name="ScanerButton" Content="Scaner" HorizontalAlignment="Left" Margin="681,334,0,0" VerticalAlignment="Top" Width="75" Click="Scaner_Click"/>
        <TextBox x:Name="UrlTextBox" HorizontalAlignment="Left" Height="281" Margin="456,10,0,0" TextWrapping="Wrap" Text="QR Image Info Content" VerticalAlignment="Top" Width="326"/>

    </Grid>
</Window>
